;; This library just contain the entry point for ppx drivers. It must
;; be linked after all other libraries and units.
(executables
 ((names (ppx_driver_runner))
  (object_public_name ppx_driver.runner)
  (libraries (ppx_driver))
  (preprocess no_preprocessing)))

(provides ppx_driver:ppx_driver_runner.cmx)

(jbuild_version jane_street)
